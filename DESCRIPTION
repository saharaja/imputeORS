Package: imputeORS
Title: Impute Missing Values in the BLS Occupational Requirements Survey
Version: 0.0.0.9000
Authors@R: 
    c(person(given = "Debjani",
           family = "Saha",
           role = c("aut", "cre"),
           #comment = c(ORCID = "YOUR-ORCID-ID"),
           email = "debjani@ruxton.ai"),
